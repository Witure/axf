/**list.wxss**/

page {  
  display: block;  
  min-height: 100%;  
  background-color: #EEE9BF;
}
.top{
  text-align: center;
  background: #EEE9BF;
  max-width: 768px;
  width: 100%;
  height: 350rpx;
  margin: 0 auto;
  color: #fff;
  padding: 24rpx 0;

}
.photo{
  width: 200rpx;
  height: 200rpx;
  margin: 0 auto;
  padding: 5px;
  border:  1px solid #ff7981;
  
  

}
.photo image{
  border-radius:50%;
  width: 100%;
  height: 100%
}
.separate {
  height: 10rpx;
  background-color: #f2f2f2;
}
.center{
  text-align: center;
  margin: 0 auto;
  padding-left: 60rpx;
}
.navs {
  display: flex;
  flex: row;
 
}

.nav-item {
  width: 160rpx;
  min-height: 150rpx;
  margin: 5rpx;
  display: flex;
  align-items: center;
  flex-direction: column;
  padding: 20rpx;
}

.nav-item .nav-image {
  width: 120rpx;
  height: 120rpx;
  padding: 30rpx;
}

.nav-item text {
  padding-top: 20rpx;
  padding-left:20rpx;
  font-size: 38rpx;
  font-family: 隶书;
}